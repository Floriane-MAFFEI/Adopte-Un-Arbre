{% extends 'base.html.twig' %}

{% block title %}Organism{% endblock %}

{% block body %}
    <h1>Organism</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ organism.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ organism.name }}</td>
            </tr>
            <tr>
                <th>Siret_insee</th>
                <td>{{ organism.siretInsee }}</td>
            </tr>
            <tr>
                <th>Adress</th>
                <td>{{ organism.adress }}</td>
            </tr>
            <tr>
                <th>Zip_code</th>
                <td>{{ organism.zipCode }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ organism.city }}</td>
            </tr>
            <tr>
                <th>Phone_number</th>
                <td>{{ organism.phoneNumber }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ organism.description }}</td>
            </tr>
            <tr>
                <th>Avatar</th>
                <td>{{ organism.avatar }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_back_organism_index') }}">back to list</a>

    <a href="{{ path('app_back_organism_edit', {'id': organism.id}) }}">edit</a>

    {{ include('back/organism/_delete_form.html.twig') }}
